git clone https://github.com/polarapu/batch-91.git
ls
cd batch-91
touch file1
ls
git status
git add file1
git status
git commit -m "label" file1
git status
git log
git show
git push

------------------------------
touch file1 file2 file3 file4
ls
git add file1 file2 file3 file4
or
git add .
or
git add *
or
git add -A

git commit -m "label" 
git push

rm file1  (to delete file1)
git status
git add file1
git commit -m "deleted file"
git push

mkdir dir1
ls
cd dir1
touch f1 f2
cd ..
git add .
git commit -m "sample dir"
git push


